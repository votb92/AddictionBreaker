<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.addictionbreaker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NotificationsActivity"></activity>
        <activity android:name=".ui.ResetProgressActivity" />
        <activity android:name=".AddAddiction" />
        <activity
            android:name=".AdictionList"
            android:label="@string/title_activity_adiction_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".ui.ConsumptionInfoActivity" />
        <activity android:name=".ui.FormActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".Receiver"/>
    </application>

</manifest>